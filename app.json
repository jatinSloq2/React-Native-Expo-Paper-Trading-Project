{
  "expo": {
    "scheme": "paperbull01",
    "name": "PaperBull",
    "slug": "PaperBull",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo_2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
